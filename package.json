{
  "name": "sre-platform-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config config/vite.config.ts",
    "build": "vite build --config config/vite.config.ts",
    "preview": "vite preview --config config/vite.config.ts",
    "mock-server": "node dev/mock-server/server.js",
    "test": "vitest --config config/vitest.config.ts",
    "lint": "eslint . --config config/eslint.config.js",
    "lint:fix": "eslint . --config config/eslint.config.js --fix",
    "lint:hardcoded": "node dev/scripts/check-hardcoded-strings.mjs",
    "lint:hardcoded:update": "node dev/scripts/check-hardcoded-strings.mjs --update",
    "format": "prettier --config config/prettier.config.cjs --write .",
    "format:check": "prettier --config config/prettier.config.cjs --check .",
    "specs:sync": "node dev/scripts/sync-module-specs.mjs",
    "specs:check": "node dev/scripts/sync-module-specs.mjs --check"
  },
  "dependencies": {
    "@google/genai": "^1.20.0",
    "antd": "^5.27.4",
    "axios": "^1.12.2",
    "cron-parser": "^5.4.0",
    "dayjs": "^1.11.18",
    "dotenv": "^17.2.3",
    "echarts": "^6.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/node": "^22.14.0",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.6.2",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^1.6.0"
  }
}
